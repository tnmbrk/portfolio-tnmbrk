{
  "name": "portfolio-tnmbrk",
  "author": {
    "name": "Toni Mubarok"
  },
  "private": true,
  "version": "0.0.0",
  "source": "src/*.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "bun run prettier && parcel",
    "re:build": "bun run clean && bun run prettier && bun run build:css && bun run build:js && parcel build",
    "build": "bun run clean && bun run build:css && bun run build:js && parcel build",
    "build:css": "tailwindcss -i ./src/css/source.css -o ./src/style.css",
    "prettier": "biome format --write ./src/",
    "clean": "rimraf dist",
    "build:js": "bun build ./src/scripts/app.js --outfile ./src/script.js --production && bun build ./node_modules/preline/dist/preline.js --outfile ./src/preline.js --production"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@parcel/optimizer-htmlnano": "^2.16.1",
    "@parcel/packager-raw-url": "^2.16.1",
    "@parcel/transformer-webmanifest": "^2.16.1",
    "@tailwindcss/forms": "^0.5.10",
    "baseline-browser-mapping": "^2.8.32",
    "parcel": "latest",
    "posthtml-doctype": "^1.1.1",
    "posthtml-include": "^2.0.1",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "js-cookie": "^3.0.5",
    "preline": "^3.2.3",
    "tailwindcss": "^4.1.13"
  },
  "nodemonConfig": {
    "ext": "js,html",
    "watch": [
      "src"
    ]
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@swc/core",
    "@tailwindcss/oxide",
    "lmdb",
    "msgpackr-extract",
    "sharp"
  ]
}
